<ehcache     
    name="MCUMConfigManager">    
    <diskStore         
        path="./cacheStore/MCUMConfigManager"/>    
    <defaultCache         
        maxEntriesLocalHeap="0"/>    
    <cache         
        name="UMConfig"         
        eternal="true"         
        maxEntriesLocalHeap="5000"         
        maxEntriesInCache="10000"         
        overflowToOffHeap="false"         
        cacheLoaderTimeoutMillis="30000">        
        <persistence             
            strategy="distributed"/>        
        <terracotta             
            consistency="STRONG">            
            <nonstop/>        
        </terracotta>    
    </cache>    
    <cache         
        name="UMCanonicalMap"         
        eternal="true"         
        maxEntriesLocalHeap="150"         
        maxEntriesInCache="500"         
        overflowToOffHeap="false"         
        cacheLoaderTimeoutMillis="30000">        
        <persistence             
            strategy="distributed"/>        
        <terracotta             
            consistency="STRONG">            
            <nonstop/>        
        </terracotta>    
    </cache>    
    <cache         
        name="UMCharacteristicAliases"         
        eternal="true"         
        maxEntriesLocalHeap="500"         
        maxEntriesInCache="2000"         
        overflowToOffHeap="false"         
        cacheLoaderTimeoutMillis="30000">        
        <persistence             
            strategy="distributed"/>        
        <terracotta             
            consistency="STRONG">            
            <nonstop/>        
        </terracotta>    
    </cache>    
    <terracottaConfig         
        url="10.129.3.118:7510,10.129.3.113:7510"         
        rejoin="true"/>
</ehcache>
