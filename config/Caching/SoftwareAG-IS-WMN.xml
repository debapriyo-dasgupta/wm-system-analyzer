<ehcache     
    name="SoftwareAG.IS.WMN.IS_Cluster"     
    monitoring="on">    
    <diskStore         
        path="./cacheStore/SoftwareAG-IS-WMN"/>    
    <cacheManagerEventListenerFactory         
        class=""         
        properties=""/>    
    <defaultCache         
        maxEntriesLocalHeap="10000"         
        timeToIdleSeconds="120"         
        timeToLiveSeconds="120"         
        maxEntriesLocalDisk="10000000"/>    
    <cache         
        name="ProcessInstance"         
        maxEntriesLocalHeap="10000"         
        timeToIdleSeconds="12000"         
        timeToLiveSeconds="12000"         
        overflowToOffHeap="false">        
        <persistence             
            strategy="distributed"/>        
        <terracotta             
            consistency="STRONG">            
            <nonstop/>        
        </terracotta>        
        <searchable>            
            <searchAttribute                 
                name="processKey"                 
                expression="value.getProcessKey()"/>            
            <searchAttribute                 
                name="instanceId"                 
                expression="value.getInstanceId()"/>            
            <searchAttribute                 
                name="status"                 
                expression="value.getStatus()"/>        
        </searchable>    
    </cache>    
    <terracottaConfig         
        url="10.129.3.118:7510,10.129.3.113:7510"         
        rejoin="true"/>
</ehcache>
